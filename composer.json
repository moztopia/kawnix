{
  "name": "kawnix/framework",
  "description": "A modular PHP framework for scalable applications.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Kawnix\\": "src/"
    }
  },
  "require": {
    "php": ">=8.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": false,
  "scripts": {
    "post-install-cmd": [
      "Kawnix\\Setup::install"
    ]
  },
  "extra": {
    "installer-paths": {
      "public/index.php": [
        "type:library"
      ]
    }
  }
}
