{
  "name": "kawnix/framework",
  "description": "A modular PHP framework for scalable applications.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Kawnix\\": "src/"
    }
  },
  "require": {
    "php": ">=8.2",
    "psr/log": "^1.1",
    "psr/simple-cache": "^1.0",
    "psr/container": "^1.1",
    "psr/http-message": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-client": "^1.0",
    "psr/event-dispatcher": "^1.0",
    "psr/link": "^1.0",
    "nesbot/carbon": "^2.66"
  },
  "config": {
    "vendor-dir": "vendor"
  },
  "scripts": {
    "post-install-cmd": [
      "Kawnix\\Setup::install"
    ]
  }
}
